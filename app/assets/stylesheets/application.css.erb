/* clear styles */

html,
body,
div,
h1,
h2,
h3,
h4,
h5,
h6,
ul,
ol,
dl,
li,
dt,
dd,
p,
blockquote,
pre,
form,
fieldset,
table,
th,
td,
legend {
    margin: 0;
    padding: 0;
}
img {
    border: 0;
    padding: 0;
    margin: 0;
    display: block;
    text-indent: -9999px;
}

::-webkit-input-placeholder { /* WebKit browsers */
    color:    #BDBDBD;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
   color:    #BDBDBD;
   opacity:  1;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
   color:    #BDBDBD;
   opacity:  1;
}
:-ms-input-placeholder { /* Internet Explorer 10+ */
   color:    #BDBDBD;
}

