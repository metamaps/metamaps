    line-height: 17px;
    width: 318px;
}
.blackBox .allMetacodes {
    padding: 5px 0;
}
.blackBox a.button {
    margin-right: 20px;
    line-height: 40px;
}
.blackBox a.button,
.blackBox input.add {
    float: left;
    margin-top: 5px;
    height: 40px;
    font-size: 17px;
    width: auto;
    padding: 0 30px;
    cursor: pointer;
    font-weight: normal;
}
.blackBox a.button:hover,
.blackBox input.add:hover {
    -webkit-box-shadow: none;
